<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

 <path>StreetLamp4.ac</path>

<animation>
  <object-name>Shine</object-name>
  <type>select</type>
    <condition>
      <greater-than>
        <property>/sim/time/sun-angle-rad</property>
        <value>1.50</value>
      </greater-than>
    </condition>
 </animation>

 <animation>
  <name>Shifting</name>
  <object-name>Glass</object-name>
 </animation>

 <animation>
  <object-name>Shifting</object-name>
  <type>textranslate</type>
    <property>/sim/time/sun-angle-rad</property>
    <step> 1.57 </step>
    <factor>0.318471338</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
  </axis>
 </animation>

</PropertyList>

