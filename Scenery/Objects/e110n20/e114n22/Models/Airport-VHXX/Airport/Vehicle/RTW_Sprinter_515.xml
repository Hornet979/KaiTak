<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>RTW_Sprinter_515.ac</path>

<!--  night emission  -->
<animation>
    <type>textranslate</type> 
    <object-name>blah</object-name> 
    <property>/sim/time/sun-angle-rad</property>
    <step> 1.57 </step>
    <factor>0.318471338</factor>
    <axis> 
      <x> -1 </x> 
      <y>  0 </y> 
    </axis> 
</animation>
<animation>
<type>material</type>
	<object-name>blah</object-name>
<condition>		
<greater-than>		
	<property>/sim/time/sun-angle-rad</property>
	<value>1.57</value>
</greater-than>
</condition>
<emission>	
	<red>0.6</red>	
	<green>0.6</green>	
	<blue>0.6</blue>
</emission>
</animation>
<animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
</animation>

</PropertyList>
