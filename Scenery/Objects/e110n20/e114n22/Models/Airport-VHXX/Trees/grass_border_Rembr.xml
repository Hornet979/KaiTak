<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

  <path>grass_border_Rembr.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>
	
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>jour</object-name>  
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
  </effect>

<animation>
    <type>material</type>
    <object-name>jour</object-name>
    <emission>
      <red> 0 </red>
      <green> 0.01 </green>
      <blue>  0.01 </blue>
    </emission>
  </animation>
	
</PropertyList>
